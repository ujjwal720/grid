    </test>
    <test name="Chrome Test - Google2">
        <parameter name="browser" value="chrome" />
        <classes>
            <class name="Practice.Goggle2" />
        </classes>
    </test>
    <test name="Firefox Test - Google2">
        <parameter name="browser" value="firefox" />
        <classes>
            <class name="Practice.Goggle2" />
        </classes>
    </test>
</suite>