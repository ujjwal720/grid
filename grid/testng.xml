<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="4">
    <test name="Chrome Test - GoogleTc">
        <parameter name="browser" value="chrome" />
        <classes>
            <class name="Practice.GoogleTc" />
